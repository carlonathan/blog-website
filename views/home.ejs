<%- include("partials/header") -%>
<h1><%= pageTitle %></h1>
<p><%= pageContent %></p>



    
<% posts.forEach(function(post) { %>
    <h1>
        <%= post.title %>
    </h1>

    <p>
        <%= post.content.substr(0, 100) + " ..." %>
        <a href="
        <%= "/posts/" + post.title %>
        ">Read More</a>
    </p>
<% }); %>


<%- include("partials/footer") -%>